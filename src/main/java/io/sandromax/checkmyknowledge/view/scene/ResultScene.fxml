<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.sandromax.checkmyknowledge.view.controller.ResultSceneController">
    <children>
        <Label fx:id="lblResults">Результаты</Label>
        <TableView fx:id="tvResults">
         <columns>
            <TableColumn fx:id="colQuestion" prefWidth="75.0" sortable="false" text="Вопрос" />
            <TableColumn fx:id="colYourAnswer" prefWidth="75.0" sortable="false" text="Ваш ответ" />
            <TableColumn fx:id="colRightAnswer" prefWidth="75.0" sortable="false" text="Правильный ответ" />
         </columns></TableView>
    </children>

</VBox>
